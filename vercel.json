{
    "version": 2,
    "builds": [
      {
        "src": "newproject/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "static/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "assets/$1"
      },
      {
        "src": "(.*)",
        "dest": "newproject/wsgi.py"
      }
    ]
  }
  